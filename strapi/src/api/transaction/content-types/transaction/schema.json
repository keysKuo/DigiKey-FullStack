{
  "kind": "collectionType",
  "collectionName": "transactions",
  "info": {
    "singularName": "transaction",
    "pluralName": "transactions",
    "displayName": "Transaction",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "transactionId": {
      "type": "string"
    },
    "customer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::customer.customer"
    },
    "total": {
      "type": "integer"
    },
    "paymentType": {
      "type": "enumeration",
      "enum": [
        "paypal",
        "stripe"
      ]
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "completed",
        "cancelled"
      ]
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-type.product-type"
    }
  }
}
